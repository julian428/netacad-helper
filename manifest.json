{
  "manifest_version": 2,
  "name": "Search Answer Extension",
  "version": "1.0",
  "description": "Search for answers in an array of objects",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
